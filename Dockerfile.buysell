# Dockerfile.buysell
FROM python:3.10-slim

WORKDIR /app

# 1) Install your buysell requirements
COPY requirements-loadbuysell.txt .
RUN pip install --no-cache-dir -r requirements-loadbuysell.txt

# 2) Copy your loader script
COPY loadbuysell.py .

# 3) Default command: run the loader
CMD ["python", "loadbuysell.py"]
