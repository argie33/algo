{
  "name": "financial-dashboard-frontend",
  "version": "1.0.3",
  "description": "React frontend for Financial Data Dashboard",
  "private": true,
  "type": "module",
  "dependencies": {
    "@aws-amplify/auth": "6.0.0",
    "@aws-amplify/core": "6.0.0",
    "@codemirror/lang-python": "^6.2.1",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/icons-material": "^5.18.0",
    "@mui/lab": "^5.0.0-alpha.177",
    "@mui/material": "^5.18.0",
    "@mui/x-date-pickers": "^6.20.2",
    "@tanstack/react-query": "^5.85.5",
    "@types/react-syntax-highlighter": "^15.5.13",
    "@uiw/react-codemirror": "^4.25.1",
    "@vitest/coverage-v8": "^3.2.4",
    "aws-amplify": "6.0.0",
    "axios": "^1.11.0",
    "date-fns": "^2.30.0",
    "numeral": "^2.0.6",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-markdown": "^10.1.0",
    "react-router-dom": "^6.30.1",
    "react-syntax-highlighter": "^15.6.6",
    "recharts": "^3.1.2",
    "web-vitals": "^5.1.0"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.10.2",
    "@babel/parser": "^7.28.3",
    "@playwright/test": "^1.55.0",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^24.3.0",
    "@types/react": "^18.3.24",
    "@vitejs/plugin-react": "^5.0.1",
    "eslint": "^8.57.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.20",
    "eslint-plugin-unused-imports": "^4.2.0",
    "glob": "^11.0.3",
    "jsdom": "^23.2.0",
    "playwright": "^1.55.0",
    "prettier": "^3.6.2",
    "puppeteer": "^24.17.0",
    "vite": "^7.1.3",
    "vitest": "^3.2.4"
  },
  "scripts": {
    "dev": "vite",
    "build": "npm run test:dep && vite build",
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 10",
    "lint:strict": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext js,jsx --fix",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "preview": "vite preview",
    "test": "vitest run src/tests/unit/ src/tests/component/ --testTimeout=15000",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage --coverage.reporter=text --coverage.reporter=html --coverage.reporter=lcov",
    "test:all": "vitest run",
    "test:fast": "vitest run src/tests/unit/components/Portfolio.test.jsx src/tests/unit/components/Settings.test.jsx src/tests/unit/components/StockDetail.test.jsx --testTimeout=10000",
    "test:unit": "vitest run src/tests/unit/ --testTimeout=10000",
    "test:contracts": "vitest run src/tests/integration/contracts/ --testTimeout=15000",
    "test:component": "vitest run src/tests/component/ --testTimeout=15000",
    "test:integration": "vitest run src/tests/integration/ --testTimeout=30000",
    "test:quality": "npm run test:dep && npm run lint && npm run test:fast && npm run test:integration && node ../test-react-build-validation.cjs",
    "test:quick": "node real-f12-test.cjs",
    "test:financial": "vitest run src/tests/unit/ src/tests/integration/ --grep=\"portfolio|calculation|financial\"",
    "test:security": "vitest run src/tests/ --grep=\"security|auth|encryption\"",
    "test:comprehensive": "npm run test:unit && npm run test:integration && npm run test:component",
    "test:ci": "npm run test:unit && npm run test:integration && npm run test:component",
    "test:critical": "npm run test:financial && npm run test:security",
    "test:validation": "PLAYWRIGHT_SKIP_VALIDATE_HOST_REQUIREMENTS=1 npx playwright test --config playwright.config.ci.js",
    "test:e2e": "PLAYWRIGHT_SKIP_VALIDATE_HOST_REQUIREMENTS=1 npx playwright test",
    "test:e2e:critical": "PLAYWRIGHT_SKIP_VALIDATE_HOST_REQUIREMENTS=1 npx playwright test critical-flows",
    "test:e2e:visual": "PLAYWRIGHT_SKIP_VALIDATE_HOST_REQUIREMENTS=1 npx playwright test --project visual-desktop",
    "test:e2e:a11y": "PLAYWRIGHT_SKIP_VALIDATE_HOST_REQUIREMENTS=1 npx playwright test --project accessibility",
    "test:e2e:perf": "PLAYWRIGHT_SKIP_VALIDATE_HOST_REQUIREMENTS=1 npx playwright test --project performance",
    "test:e2e:mobile": "PLAYWRIGHT_SKIP_VALIDATE_HOST_REQUIREMENTS=1 npx playwright test --project mobile-chrome",
    "test:e2e:report": "npx playwright show-report",
    "test:dep": "node dep-test.cjs",
    "test:security:audit": "npm audit --audit-level=high",
    "test:build": "node --version && npm ci && npm run build",
    "test:node-version": "node -e \"const v=process.version.slice(1).split('.').map(Number); if(v[0]<20||(v[0]===20&&v[1]<19)) throw new Error('Node '+process.version+' unsupported. Requires Node 20.19+ or 22.12+')\"",
    "setup-dev": "node scripts/setup-dev.js",
    "setup-prod": "node scripts/setup-prod.js",
    "start": "npm run setup-dev && npm run dev",
    "build-prod": "npm run setup-prod && npm run build",
    "dep:analyze": "node comprehensive-dep-analyzer.cjs",
    "dep:fix": "node dependency-fix-strategy.cjs",
    "dep:update": "npx npm-check-updates",
    "dep:audit": "npm audit --audit-level=high",
    "dep:validate": "node dep-test.cjs && npm run dep:analyze",
    "deploy:validate": "node deployment-validation-suite.cjs",
    "deploy:full": "npm run deploy:validate && npm run build-prod"
  },
  "overrides": {
    "use-sync-external-store": false,
    "react-is": "^18.3.1",
    "hoist-non-react-statics": "^3.3.2"
  },
  "engines": {
    "node": ">=20.19.0",
    "npm": ">=9.0.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
