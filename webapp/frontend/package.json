{
  "name": "financial-dashboard-frontend",
  "version": "1.0.3",
  "description": "React frontend for Financial Data Dashboard",
  "private": true,
  "type": "module",
  "dependencies": {
    "@aws-amplify/auth": "^6.13.1",
    "@aws-amplify/core": "^6.12.1",
    "@aws-amplify/ui-react": "^6.11.2",
    "@codemirror/lang-javascript": "^6.2.4",
    "@codemirror/lang-python": "^6.2.1",
    "@emotion/react": "^11.10.6",
    "@emotion/styled": "^11.10.6",
    "@mui/icons-material": "^5.11.11",
    "@mui/lab": "^5.0.0-alpha.124",
    "@mui/material": "^5.11.15",
    "@mui/styled-engine": "^7.1.1",
    "@mui/x-data-grid": "^6.0.3",
    "@mui/x-date-pickers": "^6.0.3",
    "@tanstack/react-query": "^4.35.3",
    "@uiw/react-codemirror": "^4.23.13",
    "aws-amplify": "^6.15.1",
    "axios": "^1.3.4",
    "chart.js": "^4.4.1",
    "date-fns": "^2.30.0",
    "lodash": "^4.17.21",
    "lucide-react": "^0.263.1",
    "numeral": "^2.0.6",
    "react": "^18.3.1",
    "react-chartjs-2": "^5.2.0",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.8.1",
    "recharts": "^2.15.4",
    "styled-components": "^5.3.9",
    "use-sync-external-store": "^1.5.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.1",
    "@types/node": "^20.19.0",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/coverage-v8": "^3.2.4",
    "eslint": "^8.55.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "eslint-plugin-unused-imports": "^4.2.0",
    "jsdom": "^23.0.1",
    "prettier": "^3.6.2",
    "vite": "^7.1.3",
    "vitest": "^3.2.4",
    "@playwright/test": "^1.40.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 10",
    "lint:strict": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext js,jsx --fix",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "preview": "vite preview",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:run": "vitest run",
    "test:unit": "vitest run src/tests/unit/",
    "test:integration": "vitest run src/tests/integration/",
    "test:component": "vitest run src/tests/unit/components/ src/tests/component/ src/tests/component-integration/",
    "test:e2e": "playwright test src/tests/e2e/",
    "test:e2e:headed": "playwright test src/tests/e2e/ --headed",
    "test:financial": "vitest run src/tests/unit/ src/tests/integration/ --grep=\"portfolio|calculation|financial\"",
    "test:security": "vitest run src/tests/ --grep=\"security|auth|encryption\"",
    "test:comprehensive": "npm run test:unit && npm run test:integration && npm run test:component && npm run test:e2e",
    "test:ci": "npm run test:unit && npm run test:integration && npm run test:component",
    "test:critical": "npm run test:financial && npm run test:security",
    "test:dep": "npm audit --audit-level=moderate",
    "test:quality": "npm run lint && npm run format:check",
    "test:security:audit": "npm audit --audit-level=high",
    "test:performance": "vitest run src/tests/performance/",
    "test:all": "npm run test:dep && npm run test:quality && npm run test:unit && npm run test:integration && npm run test:e2e && npm run test:security && npm run test:performance && npm run test:component",
    "test:build": "node --version && npm ci && npm run build",
    "test:node-version": "node -e \"const v=process.version.slice(1).split('.').map(Number); if(v[0]<20||(v[0]===20&&v[1]<19)) throw new Error('Node '+process.version+' unsupported. Requires Node 20.19+ or 22.12+')\"",
    "setup-dev": "node scripts/setup-dev.js",
    "setup-prod": "node scripts/setup-prod.js",
    "start": "npm run setup-dev && npm run dev",
    "build-prod": "npm run setup-prod && npm run build"
  },
  "engines": {
    "node": ">=20.19.0",
    "npm": ">=9.0.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
