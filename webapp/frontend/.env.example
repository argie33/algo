# ProTrade Analytics Environment Configuration Example
# Copy this file to .env.local and configure your values

# ====================
# APPLICATION CONFIG
# ====================
VITE_APP_VERSION=1.0.0
VITE_APP_URL=http://localhost:3000
VITE_SUPPORT_EMAIL=support@protrade.com
VITE_SUPPORT_PHONE=+1-800-PROTRADE

# ====================
# AWS CONFIGURATION
# ====================
VITE_AWS_REGION=us-east-1

# API Gateway
VITE_API_BASE_URL=https://your-api-gateway-url.execute-api.us-east-1.amazonaws.com
VITE_API_VERSION=v1
VITE_API_TIMEOUT=30000
VITE_API_RETRY_ATTEMPTS=3
VITE_API_RETRY_DELAY=1000

# Cognito Authentication
VITE_COGNITO_USER_POOL_ID=us-east-1_XXXXXXXXX
VITE_COGNITO_CLIENT_ID=1234567890abcdefghijklmnop
VITE_COGNITO_DOMAIN=your-cognito-domain.auth.us-east-1.amazoncognito.com
VITE_COGNITO_REDIRECT_SIGN_IN=http://localhost:3000
VITE_COGNITO_REDIRECT_SIGN_OUT=http://localhost:3000

# S3 Storage
VITE_S3_BUCKET=your-s3-bucket-name
VITE_S3_REGION=us-east-1

# Lambda Functions
VITE_LAMBDA_REGION=us-east-1

# ====================
# WEBSOCKET CONFIGURATION
# ====================
# IMPORTANT: Configure this for live data service to work
# Real deployed WebSocket endpoint (replace with your own)
VITE_WS_URL=wss://ckzvfd1ds3.execute-api.us-east-1.amazonaws.com/dev
VITE_WEBSOCKET_ENABLED=true
VITE_WS_RECONNECT_INTERVAL=5000
VITE_WS_MAX_RECONNECT=10
VITE_WS_HEARTBEAT_INTERVAL=30000
VITE_WS_CONNECTION_TIMEOUT=10000
VITE_WS_MESSAGE_TIMEOUT=5000
VITE_WS_AUTO_CONNECT=true
VITE_WS_HEALTH_CHECK=true
VITE_WS_HEALTH_CHECK_INTERVAL=5000

# ====================
# EXTERNAL API KEYS
# ====================
# Alpaca Trading API (Paper Trading)
VITE_ALPACA_BASE_URL=https://paper-api.alpaca.markets
VITE_ALPACA_DATA_URL=https://data.alpaca.markets
VITE_ALPACA_WS_URL=wss://stream.data.alpaca.markets
VITE_ALPACA_API_KEY_ID=your_alpaca_api_key_id
VITE_ALPACA_SECRET_KEY=your_alpaca_secret_key
VITE_ALPACA_IS_PAPER=true

# Polygon.io Market Data
VITE_POLYGON_BASE_URL=https://api.polygon.io
VITE_POLYGON_WS_URL=wss://socket.polygon.io
VITE_POLYGON_API_KEY=your_polygon_api_key

# Financial Modeling Prep
VITE_FMP_BASE_URL=https://financialmodelingprep.com/api
VITE_FMP_API_KEY=your_fmp_api_key

# Finnhub
VITE_FINNHUB_BASE_URL=https://finnhub.io/api/v1
VITE_FINNHUB_WS_URL=wss://ws.finnhub.io
VITE_FINNHUB_API_KEY=your_finnhub_api_key

# Alpha Vantage
VITE_ALPHA_VANTAGE_BASE_URL=https://www.alphavantage.co
VITE_ALPHA_VANTAGE_API_KEY=your_alpha_vantage_api_key

# News API
VITE_NEWS_API_BASE_URL=https://newsapi.org/v2
VITE_NEWS_API_KEY=your_news_api_key

# Yahoo Finance (no key required)
VITE_YAHOO_FINANCE_BASE_URL=https://query1.finance.yahoo.com

# ====================
# DATABASE CONFIGURATION
# ====================
VITE_RDS_HOST=your-rds-endpoint.region.rds.amazonaws.com
VITE_RDS_PORT=5432
VITE_RDS_DATABASE=protrade
VITE_RDS_SSL=true

# Redis Configuration
VITE_REDIS_HOST=your-redis-cluster.cache.amazonaws.com
VITE_REDIS_PORT=6379
VITE_REDIS_SSL=true

# ====================
# FEATURE FLAGS
# ====================
# Authentication Features
VITE_FEATURE_AUTH=true
VITE_FEATURE_COGNITO=true
VITE_FEATURE_OAUTH=false
VITE_FEATURE_BIOMETRIC=false

# Trading Features
VITE_FEATURE_TRADING=true
VITE_FEATURE_PAPER_TRADING=true
VITE_FEATURE_REAL_TRADING=false
VITE_FEATURE_CRYPTO_TRADING=false
VITE_FEATURE_OPTIONS_TRADING=false

# Data Features
VITE_FEATURE_REALTIME_DATA=true
VITE_FEATURE_HISTORICAL_DATA=true
VITE_FEATURE_NEWS_DATA=true
VITE_FEATURE_SOCIAL_SENTIMENT=false

# AI Features
VITE_FEATURE_AI=true
VITE_FEATURE_AI_SIGNALS=true
VITE_FEATURE_AI_PORTFOLIO=false
VITE_FEATURE_AI_RISK=false
VITE_FEATURE_AI_SENTIMENT=false

# Analytics Features
VITE_FEATURE_ANALYTICS=true
VITE_FEATURE_PERFORMANCE=true
VITE_FEATURE_RISK=true
VITE_FEATURE_BACKTESTING=false

# UI Features
VITE_FEATURE_DARK_MODE=true
VITE_FEATURE_CUSTOM_THEMES=false
VITE_FEATURE_ACCESSIBILITY=true
VITE_FEATURE_MOBILE=true

# ====================
# PERFORMANCE CONFIG
# ====================
# Caching
VITE_CACHE_ENABLED=true
VITE_CACHE_MARKET_DATA_TTL=60000
VITE_CACHE_PORTFOLIO_TTL=300000
VITE_CACHE_NEWS_TTL=900000
VITE_CACHE_STATIC_TTL=3600000

# Rate Limiting
VITE_RATE_LIMIT_ENABLED=true
VITE_RATE_LIMIT_PER_MINUTE=100
VITE_RATE_LIMIT_PER_HOUR=1000
VITE_RATE_LIMIT_PER_DAY=10000

# ====================
# SECURITY CONFIG
# ====================
VITE_ENCRYPTION_ENABLED=true
VITE_ENCRYPTION_ALGORITHM=AES-256-GCM
VITE_CSP_ENABLED=true
VITE_CSP_REPORT_ONLY=false
VITE_SESSION_TIMEOUT=3600000
VITE_SESSION_RENEW_BEFORE=300000
VITE_MAX_CONCURRENT_SESSIONS=3

# ====================
# MONITORING CONFIG
# ====================
# Logging
VITE_LOGGING_ENABLED=true
VITE_LOG_LEVEL=info
VITE_LOG_CONSOLE=true
VITE_LOG_REMOTE=false

# Error Tracking
VITE_ERROR_TRACKING=false
VITE_SENTRY_DSN=your_sentry_dsn

# Analytics
VITE_ANALYTICS_ENABLED=false
VITE_GA_TRACKING_ID=GA-XXXXXXXXX

# ====================
# DEVELOPMENT CONFIG
# ====================
VITE_DEBUG_MODE=false
VITE_MOCK_DATA=false
VITE_TEST_MODE=false
VITE_DEV_TOOLS=false