{
  "name": "financial-dashboard-lambda",
  "version": "1.0.0",
  "description": "Serverless Lambda function for Financial Data Dashboard API",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "test": "jest --testPathPattern=tests/unit --verbose",
    "test:unit": "jest --config jest.config.unit.js --verbose",
    "test:unit:ci": "jest --config jest.config.unit.js --verbose --coverage --passWithNoTests",
    "test:integration": "jest --config jest.integration.config.js --testPathPattern=tests/integration --verbose --runInBand --detectOpenHandles --forceExit",
    "test:integration:real": "node scripts/load-test-env.js && jest --config jest.config.integration.js --verbose --runInBand --detectOpenHandles --forceExit",
    "test:integration:embedded": "npx jest tests/integration/embedded-real-integration.test.js --verbose --maxWorkers=1 --testTimeout=30000 --forceExit",
    "upload:unit-tests": "bash scripts/upload-unit-test-workflow.sh",
    "setup:integration-auth": "node scripts/setup-integration-auth.js",
    "test:integration:complete": "bash scripts/run-complete-integration-tests.sh",
    "test:integration:ci": "jest --config jest.integration.config.js --runInBand --detectOpenHandles --forceExit",
    "test:integration:aws": "jest --config jest.integration.config.js --testPathPattern=\"database-integration|auth-integration|portfolio-integration\" --runInBand --detectOpenHandles --forceExit",
    "test:all": "jest --verbose --runInBand --detectOpenHandles --forceExit",
    "test:watch": "jest --watch --testPathPattern=tests/unit",
    "test:coverage": "jest --coverage --testPathPattern=tests/unit",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "package": "npm prune --production && zip -r function.zip . -x '*.git*' 'node_modules/.cache/*' 'tests/*' 'coverage/*'",
    "deploy-package": "npm run package && aws lambda update-function-code --function-name financial-dashboard-api --zip-file fileb://function.zip"
  },
  "dependencies": {
    "@alpacahq/alpaca-trade-api": "^3.1.3",
    "@aws-sdk/client-cloudformation": "^3.846.0",
    "@aws-sdk/client-cloudwatch": "^3.848.0",
    "@aws-sdk/client-cognito-identity-provider": "^3.840.0",
    "@aws-sdk/client-ecs": "^3.844.0",
    "@aws-sdk/client-lambda": "^3.844.0",
    "@aws-sdk/client-secrets-manager": "^3.846.0",
    "@aws-sdk/client-ses": "^3.848.0",
    "@aws-sdk/client-sns": "^3.848.0",
    "@aws-sdk/client-ssm": "^3.848.0",
    "@aws-sdk/rds-signer": "^3.454.0",
    "aws-jwt-verify": "^5.1.0",
    "bcrypt": "^6.0.0",
    "cors": "^2.8.5",
    "dompurify": "^3.0.5",
    "dotenv": "^16.6.1",
    "express": "^4.18.2",
    "express-rate-limit": "^6.7.0",
    "express-slow-down": "^1.6.0",
    "helmet": "^7.1.0",
    "ioredis": "^5.3.2",
    "jsdom": "^23.0.1",
    "jsonwebtoken": "^9.0.2",
    "ml-matrix": "^6.12.1",
    "morgan": "^1.10.0",
    "node-fetch": "^3.3.2",
    "ollama": "^0.5.0",
    "pg": "^8.16.3",
    "qrcode": "^1.5.3",
    "serverless-http": "^3.2.0",
    "speakeasy": "^2.0.0",
    "validator": "^13.9.0",
    "winston": "^3.17.0",
    "ws": "^8.18.0",
    "axios": "^1.10.0"
  },
  "devDependencies": {
    "@babel/core": "^7.28.0",
    "@babel/plugin-transform-modules-commonjs": "^7.27.1",
    "@babel/preset-env": "^7.28.0",
    "@eslint/js": "^9.31.0",
    "aws-sdk": "^2.1692.0",
    "babel-jest": "^30.0.5",
    "eslint": "^9.31.0",
    "express-validator": "^7.2.1",
    "jest": "^29.7.0",
    "jest-junit": "^16.0.0",
    "sqlite": "^5.1.1",
    "sqlite3": "^5.1.7",
    "supertest": "^6.3.4"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "serverless",
    "lambda",
    "financial",
    "dashboard",
    "api"
  ],
  "author": "Financial Dashboard Team",
  "license": "MIT"
}
