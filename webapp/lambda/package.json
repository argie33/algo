{
  "name": "financial-dashboard-lambda",
  "version": "1.0.0",
  "description": "Serverless Lambda function for Financial Data Dashboard API",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "test": "jest",
    "package": "npm prune --production && zip -r function.zip . -x '*.git*' 'node_modules/.cache/*' 'tests/*' 'coverage/*'",
    "deploy-package": "npm run package && aws lambda update-function-code --function-name financial-dashboard-api --zip-file fileb://function.zip"
  },
  "dependencies": {
    "@alpacahq/alpaca-trade-api": "^3.0.3",
    "@aws-sdk/client-cloudformation": "^3.846.0",
    "@aws-sdk/client-cognito-identity-provider": "^3.840.0",
    "@aws-sdk/client-ecs": "^3.844.0",
    "@aws-sdk/client-lambda": "^3.844.0",
    "@aws-sdk/client-secrets-manager": "^3.846.0",
    "@aws-sdk/client-ssm": "^3.848.0",
    "@aws-sdk/rds-signer": "^3.454.0",
    "aws-jwt-verify": "^5.1.0",
    "axios": "^1.6.0",
    "cors": "^2.8.5",
    "dompurify": "^3.0.5",
    "dotenv": "^16.6.1",
    "express": "^4.18.2",
    "express-rate-limit": "^6.7.0",
    "express-slow-down": "^1.6.0",
    "helmet": "^7.1.0",
    "ioredis": "^5.3.2",
    "jsdom": "^23.0.1",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "ollama": "^0.5.0",
    "pg": "^8.16.3",
    "qrcode": "^1.5.3",
    "serverless-http": "^3.2.0",
    "speakeasy": "^2.0.0",
    "validator": "^13.9.0",
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "express-validator": "^7.2.1",
    "jest": "^29.7.0",
    "supertest": "^6.3.4"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "serverless",
    "lambda",
    "financial",
    "dashboard",
    "api"
  ],
  "author": "Financial Dashboard Team",
  "license": "MIT"
}
